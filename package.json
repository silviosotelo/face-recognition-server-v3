{
    "name": "face-recognition-backend-v3",
    "version": "4.1.0",
    "description": "Sistema de reconocimiento facial - GPU/CUDA + HNSW + Redis + PostgreSQL - Escala 1M+ caras",
    "main": "app.js",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "start": "node app.js",
        "start:cluster": "pm2 start ecosystem.config.js",
        "start:gpu": "TF_FORCE_GPU_ALLOW_GROWTH=true node app.js",
        "dev": "nodemon app.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "migrate": "node scripts/migrate.js",
        "seed": "node scripts/seed.js",
        "migrate:from-sqlite": "node scripts/migrate-sqlite-to-postgres.js",
        "build:index": "node scripts/build-hnsw-index.js",
        "check:gpu": "node scripts/gpu-check.js",
        "pm2:start": "pm2 start ecosystem.config.js",
        "pm2:stop": "pm2 stop face-recognition",
        "pm2:reload": "pm2 reload face-recognition",
        "pm2:logs": "pm2 logs face-recognition",
        "pm2:monit": "pm2 monit"
    },
    "dependencies": {
        "express": "^4.18.2",
        "@vladmandic/face-api": "^1.7.15",
        "canvas": "^2.11.2",
        "pg": "^8.11.3",
        "sharp": "^0.32.6",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "compression": "^1.7.4",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "winston": "^3.11.0",
        "node-cache": "^5.1.2",
        "dotenv": "^16.3.1",
        "validator": "^13.11.0",
        "@tensorflow/tfjs-node": "^4.22.0",
        "@tensorflow/tfjs-node-gpu": "^4.22.0",
        "hnswlib-node": "^3.0.0",
        "ioredis": "^5.3.2",
        "prom-client": "^15.1.0",
        "bull": "^4.12.2",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "eslint": "^8.53.0",
        "@types/node": "^20.8.10",
        "pm2": "^5.3.1",
        "sqlite3": "^5.1.6"
    },
    "keywords": [
        "reconocimiento-facial",
        "face-recognition",
        "ai",
        "computer-vision",
        "biometrics",
        "security",
        "gpu",
        "cuda",
        "tensorflow",
        "hnsw",
        "redis",
        "postgresql"
    ],
    "author": "Silvio Sotelo",
    "license": "MIT",
    "overrides": {
        "@tensorflow/tfjs": "^4.22.0",
        "@tensorflow/tfjs-backend-cpu": "^4.22.0",
        "@tensorflow/tfjs-backend-wasm": "^4.22.0",
        "@tensorflow/tfjs-backend-webgl": "^4.22.0",
        "@tensorflow/tfjs-backend-webgpu": "4.22.0",
        "@tensorflow/tfjs-converter": "^4.22.0",
        "@tensorflow/tfjs-core": "^4.22.0",
        "@tensorflow/tfjs-data": "^4.22.0",
        "@tensorflow/tfjs-layers": "^4.22.0",
        "@tensorflow/tfjs-node": "^4.22.0",
        "@tensorflow/tfjs-node-gpu": "^4.22.0"
    }
}